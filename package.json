{
  "name": "nunjucks-intl",
  "version": "1.0.0",
  "description": "Nunjucks helpers for internationalization with format.js.",
  "keywords": [
    "intl",
    "i18n",
    "nunjucks"
  ],
  "author": "Konstantin Burkalev <KSDaemon@ya.ru>",
  "contributors": [
    "Eric Ferraiuolo <edf@ericf.me>",
    "Drew Folta <drew@folta.net>",
    "Caridy Patino <caridy@gmail.com>",
    "Anthony Pipkin <a.pipkin@yahoo.com>"
  ],
  "bugs": {
    "url": "https://github.com/KSDaemon/nunjucks-intl/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:KSDaemon/nunjucks-intl.git"
  },
  "main": "index.js",
  "jsnext:main": "src/main.js",
  "browser": {
    "./lib/locales": false,
    "./lib/locales.js": false
  },
  "dependencies": {
    "intl-format-cache": "^4.0.1",
    "intl-messageformat": "^5.0.1",
    "intl-relativeformat": "^6.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.4.4",
    "babelify": "^10.0.0",
    "benchmark": "^2.1.4",
    "expect.js": "^0.3.1",
    "grunt": "^1.0.1",
    "grunt-babel": "^8.0.0",
    "grunt-benchmark": "^1.0.0",
    "grunt-bundle-jsnext-lib": "^0.5.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-jshint": "^2.0.0",
    "grunt-contrib-uglify": "^4.0.0",
    "grunt-extract-cldr-data": "^4.1.0",
    "grunt-json-remove-fields": "git://github.com/ericf/grunt-json-remove-fields#improve-files-and-output",
    "intl": "^1.2.5",
    "istanbul": "^0.4.5",
    "load-grunt-tasks": "^5.0.0",
    "mocha": "^6.0.0",
    "nunjucks": "^3.0.0",
    "xunit-file": "1.0.0"
  },
  "scripts": {
    "prepublish11": "grunt",
    "test": "istanbul cover -x lib/locales.js -x lib/en.js -- ./node_modules/mocha/bin/mocha --exit --require @babel/register -R spec tests/runner.js",
    "travis": "istanbul cover -x lib/locales.js -x lib/en.js -- _mocha --require @babel/register tests/runner.js --reporter spec"
  },
  "homepage": "https://github.com/KSDaemon/nunjucks-intl",
  "directories": {
    "test": "tests"
  },
  "license": "BSD"
}
